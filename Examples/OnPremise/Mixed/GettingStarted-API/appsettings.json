{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  // For a sample configuration file demonstrating all available options, see 
  // https://github.com/51Degrees/device-detection-dotnet-examples/blob/main/Examples/sample-configuration.json
  "PipelineOptions": {
    "Elements": [
      {
        // Device Detection element
        "BuilderName": "DeviceDetectionHashEngineBuilder",
        // Make sure you put 51Degrees-EnterpriseV4.1.hash or 51Degrees-LiteV4.1.hash data file
        //into ip-intelligence-data directory
        "BuildParameters": {
          "DataFile": "51Degrees-EnterpriseV41.hash",
          "CreateTempDataCopy": false,
          "AutoUpdate": false,
          "PerformanceProfile": "MaxPerformance",
          "DataFileSystemWatcher": false,
          "DataUpdateOnStartUp": false,
          // Properties for device detection
          "Properties": "DeviceId,JavascriptHardwareProfile,HardwareVendor,HardwareModel,HardwareName,IsMobile,JavascriptGetHighEntropyValues,Promise,Fetch,DeviceType,PlatformVendor,PlatformName,PlatformVersion,BrowserVendor,BrowserName,BrowserVersion,ScreenPixelsWidth,ScreenPixelsHeight,ScreenPixelsWidthJavascript,ScreenPixelsHeightJavascript"
        }
      },
      {
        // IP Intelligence element  
        "BuilderName": "IpiOnPremiseEngineBuilder",
        "BuildParameters": {
          //Make sure this data file is in the ip-intelligence-data directory
          "DataFile": "51Degrees-EnterpriseIpiV41.ipi",
          "CreateTempDataCopy": false,
          "AutoUpdate": false,
          "PerformanceProfile": "MaxPerformance",
          "DataFileSystemWatcher": false,
          "DataUpdateOnStartUp": false,
          // Properties for IP intelligence
          "Properties": "RegisteredName,RegisteredOwner,RegisteredCountry,IpRangeStart,IpRangeEnd,Country,CountryCode,CountryCode3,Region,State,Town,Latitude,Longitude,Areas,AccuracyRadius,TimeZoneOffset"
        }
      },
      {
        // Required to add Javascript and JSON end points for 51Degrees.core.js and 
        "BuilderName": "JsonBuilderElement"
      }
    ],
    // Disable client-side evidence to avoid JSON serialization issues with IPAddress
    "ClientSideEvidenceEnabled": false,
    "UseAsyncScript": false
  }
}
