{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  // For a sample configuration file demonstrating all available options, see 
  // https://github.com/51Degrees/device-detection-dotnet-examples/blob/main/Examples/sample-configuration.json
  "PipelineOptions": {
    "Elements": [
      {
        "BuilderName": "IpiOnPremiseEngineBuilder",
        "BuildParameters": {
          "DataFile": "51Degrees-EnterpriseIpiV41-AllProperties.ipi",
          "CreateTempDataCopy": false,
          "AutoUpdate": false,
          "PerformanceProfile": "MaxPerformance",
          "DataFileSystemWatcher": false,
          "DataUpdateOnStartUp": false,
          // Include all available properties
          "Properties": "AccuracyRadiusMax,AccuracyRadiusMin,Areas,AsnName,AsnNumber,ConnectionType,ContinentCode2,ContinentName,Country,CountryCode,CountryCode3,County,CurrencyCode,DialCode,HumanProbability,IpRangeEnd,IpRangeStart,IsBroadband,IsCellular,IsEu,IsHosted,IsProxy,IsPublicRouter,IsTor,IsVPN,LanguageCode,Latitude,LocationConfidence,Longitude,Mcc,Region,RegisteredCountry,RegisteredName,RegisteredOwner,State,Suburb,TimeZoneIana,TimeZoneOffset,Town,ZipCode"
        }
      }
    ],
    // Disable client-side evidence to avoid JSON serialization issues with IPAddress
    "ClientSideEvidenceEnabled": false,
    "UseAsyncScript": false
  }
}
